{
  "name": "enlightened-structures",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "./node_modules/.bin/harp compile src dist",
    "data": "./node_modules/.bin/coffee data.coffee",
    "dev": "npm start & npm run watch-data & npm run watch",
    "h2j": "./node_modules/.bin/html2jade",
    "shipit": "npm test && git push origin head",
    "start": "./node_modules/.bin/harp server --port ${PORT:=9000} src",
    "test": "npm run build",
    "watch": "browser-sync start --port 8080 --proxy 'localhost:9000' --files 'src/*.json, src/*.jade, src/styles/*, src/img/*'",
    "watch-data": "./node_modules/.bin/nodemon --watch data.coffee data.coffee"
  },
  "author": "Harlan T Wood",
  "license": "",
  "devDependencies": {
    "browser-sync": "^1.3.7",
    "coffee-script": "^1.9.1",
    "html2jade": "^0.8.3",
    "lightsaber": "^0.4.1"
  },
  "dependencies": {
    "harp": "^0.13.0"
  },
  "engines": {
    "node": "~0.10.38",
    "npm": "^2.7.6"
  }
}

